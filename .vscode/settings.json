{
    "nuxt.isNuxtApp": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.fixAll.stylelint": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "[html,javascript,json,xml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "stylelint.validate": ["css", "scss", ".module.scss"],
    "eslint.alwaysShowStatus": true,
    "eslint.debug": true,
    "eslint.format.enable": true,
    "eslint.validate": ["javascript", "html", "vue", "json", "javascriptreact", "typescript", "typescriptreact"],
    "eslint.options": {
        "extensions": [".html", ".js", ".vue", ".jsx", "json", ".tsx"]
    },
    "jest.runAllTestsFirst": false,
    "jest.autoEnable": false,
    "jest.showCoverageOnLoad": false,
    "cSpell.words": [
        "adipiscing",
        "aliqua",
        "aliquam",
        "aliquip",
        "amet",
        "blazwers",
        "childrenswear",
        "clothingegy",
        "clothingtastic",
        "desi",
        "dolore",
        "duis",
        "Duisbarg",
        "eiusmod",
        "elit",
        "enim",
        "fashadil",
        "fashnetic",
        "Fendi",
        "fontsource",
        "Frenaldo",
        "fullscreen",
        "Gucchi",
        "headlessui",
        "hoppister",
        "incididunt",
        "Jiniya",
        "labore",
        "laboris",
        "megamenu",
        "nbsp",
        "nostrud",
        "overlayscrollbars",
        "Polarised",
        "quis",
        "REDQ",
        "Scelerisque",
        "segoe",
        "sholy",
        "shoozly",
        "shosio",
        "shovia",
        "Shurgs",
        "signin",
        "sollicitudin",
        "Solly",
        "speciality",
        "swiper",
        "tailwindcss",
        "tassell",
        "tempor",
        "Tissot",
        "ullamco",
        "ultrices",
        "upto",
        "Veni",
        "veniam",
        "Vici",
        "Vidi",
        "womens"
    ],
    "i18n-ally.enabledFrameworks": ["next-intl", "vscode"],
    "i18n-ally.localesPaths": ["public/messages"],
    "i18n-ally.sourceLanguage": "en",
    "i18n-ally.keystyle": "nested",
    "i18n-ally.regex.usageMatchAppend": ["t[a-z]?\\('([^']+)'\\)|t[a-z]?\\(\"([^\"]+)\"\\)"]
}
